#All Marketplace + ALL
#ALL Sales and Supply - No Lag
allallno <- read.csv("AllMkt_All_NoLag.csv")
allallno_regress <- lm(allallno$Sales ~ allallno$SRPVs)
summary(allallno_regress)

#ALL Sales and Supply - 1M
allall1 <- read.csv("AllMkt_All_1M.csv")
allall1_regress <- lm(allall1$Sales ~ allall1$SRPVs)
summary(allall1_regress)

#ALL Sales and Supply - 2M
allall2 <- read.csv("AllMkt_All_2M.csv")
allall2_regress <- lm(allall2$Sales ~ allall2$SRPVs)
summary(allall2_regress)

#ALL Sales and Supply - 3M
allall3 <- read.csv("AllMkt_All_3M.csv")
