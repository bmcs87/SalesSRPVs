#Macro Sales and Supply - No Lag
macro <- read.csv("Sales_Supply.csv")
macro_regress <- lm(macro$Vehicle.Sales ~ macro$SRPVs)
summary(macro_regress)

#Macro Sales and Supply - One month time lapse
macro <- read.csv("Sales_Supply_1monthDelay.csv")
macro_regress <- lm(macro$Vehicle.Sales ~ macro$SRPVs)
summary(macro_regress)

#Macro Sales and Supply - Two month time lapse
macro <- read.csv("Sales_Supply_2monthDelay.csv")
macro_regress <- lm(macro$Vehicle.Sales ~ macro$SRPVs)
summary(macro_regress)

#Macro Sales and Supply - Three month time lapse
macro <- read.csv("Sales_Supply_3monthDelay.csv")
macro_regress <- lm(macro$Vehicle.Sales ~ macro$SRPVs)
summary(macro_regress)

#Macro Sales and Supply - Four month time lapse
macro <- read.csv("Sales_Supply_4monthDelay.csv")
macro_regress <- lm(macro$Vehicle.Sales ~ macro$SRPVs)
summary(macro_regress)
