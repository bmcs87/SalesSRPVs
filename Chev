#Chev Sales and Supply
Chev <- read.csv("SS_Chev.csv")
Chev_regress <- lm(Chev$Vehicle.Sales ~ Chev$SRPVs)
summary(Chev_regress)


#Chev Sales and Supply - One month time lapse
Chev1m <- read.csv("SS_Chev_1monthDelay.csv")
Chev_regress1m <- lm(Chev1m$Vehicle.Sales ~ Chev1m$SRPVs)
summary(Chev_regress1m)


#Chev Sales and Supply - Two month time lapse
Chev2m <- read.csv("SS_Chev_2monthDelay.csv")
Chev_regress2m <- lm(Chev2m$Vehicle.Sales ~ Chev2m$SRPV)
summary(Chev_regress2m)


#Chev Sales and Supply - Three month time lapse
Chev3m <- read.csv("SS_Chev_3monthDelay.csv")
Chev_regress3m <- lm(Chev3m$Vehicle.Sales ~ Chev3m$SRPVs)
summary(Chev_regress3m)


#Chev Sales and Supply - Four month time lapse
Chev4m <- read.csv("SS_Chev_4monthDelay.csv")
Chev_regress4m <- lm(Chev4m$Vehicle.Sales ~ Chev4m$SRPVs)
summary(Chev_regress4m)
