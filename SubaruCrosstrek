
#Subaru Crosstrek Model Sales and Supply
model <- read.csv("SS_Models_Crosstrek.csv")
model_regress <- lm(model$Vehicle.Sales ~ model$SRPVs)
summary(model_regress)


#Subaru Crosstrek Model Sales and Supply - One month time lapse
model1m <- read.csv("SS_Models_Crosstrek_1MonthDelay.csv")
model1m_regress <- lm(model1m$Vehicle.Sales ~ model1m$SRPVs)
summary(model1m_regress)


#Subaru Crosstrek Model Sales and Supply - Two month time lapse
model2m <- read.csv("SS_Models_Crosstrek_2MonthDelay.csv")
model2m_regress <- lm(model2m$Vehicle.Sales ~ model2m$SRPVs)
summary(model2m_regress)


#Subaru Crosstrek Model Sales and Supply - Three month time lapse
model3m <- read.csv("SS_Models_Crosstrek_3MonthDelay.csv")
model3m_regress <- lm(model3m$Vehicle.Sales ~ model3m$SRPVs)
summary(model3m_regress)


#Subaru Crosstrek Model Sales and Supply - Four month time lapse
model4m <- read.csv("SS_Models_Crosstrek_4MonthDelay.csv")
model4m_regress <- lm(model4m$Vehicle.Sales ~ model4m$SRPVs)
summary(model4m_regress)
